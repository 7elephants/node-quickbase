{
  "name": "quickbase",
  "version": "6.0.0",
  "description": "A lightweight, typed, promise-based Quickbase API, autogenerated from the OpenAPI spec",
  "keywords": [
    "QuickBase",
    "quickbase",
    "Quick Base",
    "quick base",
    "qb",
    "api"
  ],
  "homepage": "https://github.com/tflanagan/node-quickbase",
  "bugs": {
    "url": "https://github.com/tflanagan/node-quickbase/issues",
